<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.e_votingapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon_3_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EVotingApp">
        <activity android:name=".already_voted"></activity>
        <activity android:name=".thanks_for_voting" />
        <activity android:name=".results" />
        <activity android:name=".candidate_info_to_choose" />
        <activity android:name=".unregisteredVoter" />
        <activity android:name=".voter_home" />
        <activity android:name=".voterLoginOTP" />
        <activity android:name=".votersList" />
        <activity android:name=".newVoter" />
        <activity android:name=".candidatesList" />
        <activity android:name=".newCandidate" />
        <activity android:name=".adminHome" />
        <activity android:name=".adminLogin" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>